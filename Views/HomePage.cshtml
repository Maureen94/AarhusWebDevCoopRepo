@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.HomePage>
@using ContentModels = Umbraco.Web.PublishedModels;
@{
    Layout = "Master.cshtml";
}


<div class="container">
    <img src="@Model.Logo.Url" width="100%" height="60%" style="overflow:hidden" />
    <div class="jumbotron">
        <h1>@Model.SiteTitle</h1>
        <p>@Model.MainContent</p>
    </div>
</div>



@Umbraco.RenderMacro("listNewestProjects")
